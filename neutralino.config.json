{
  "$schema": "https://raw.githubusercontent.com/neutralinojs/neutralinojs/main/schemas/neutralino.config.schema.json",
  "applicationId": "com.misosouptgit.notdotblur",
  "version": "1.0.0",
  "defaultMode": "window",
  "port": 0,
  "documentRoot": "/resources/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "tokenSecurity": "one-time",
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "debug.log"
  ],
  "globalVariables": {
    "TEST1": "Hello",
    "TEST2": [
      2,
      4,
      5
    ],
    "TEST3": {
      "value1": 10,
      "value2": {}
    }
  },
  
  "modes": {
    "window": {
      "title": "NotDotBlur",
      "width": 1000,
      "height": 700,
      "center": true,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/resources/favicon.ico",
      "enableInspector": false,
      "enableDevTools": false,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": false,
      "exitProcessOnClose": true
    },
    "browser": {
      "globalVariables": {
        "TEST": "Test value browser"
      },
      "nativeBlockList": []
    },
    "cloud": {
      "url": "/resources/#cloud",
      "nativeAllowList": [
        "app.*"
      ]
    },
    "chrome": {
      "width": 800,
      "height": 500,
      "args": "--user-agent=\"Neutralinojs chrome mode\""
    }
  },
  "permissions": [
    "filesystem.readFile",
    "filesystem.writeFile",
    "os.showSaveDialog",
    "os.showOpenDialog",
    "os.showMessageBox"
  ],
  "applicationName": "NotDotBlur",
  "applicationIcon": "resources/icon.png",
  "description": "NotDotBlur",
  "author": "MisoPy",
  "cli": {
    "binaryName": "NotDotBlur",
    "resourcesPath": "/resources/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/resources/js/neutralino.js",
    "binaryVersion": "6.4.0",
    "clientVersion": "6.4.0"
  }
}